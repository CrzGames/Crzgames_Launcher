{
  "name": "@crzgames/launcher",
  "version": "1.14.0",
  "private": true,
  "type": "module",
  "scripts": {
    "web:run:develop": "nuxi dev --dotenv .env.development --no-clear --port=3555",
    "web:run:staging": "nuxi dev --dotenv .env.staging --no-clear --port=3555",
    "web:run:prod": "nuxi dev --dotenv .env.production --no-clear --port=3555",
    "web:build:static:develop": "nuxi generate --dotenv .env.development",
    "web:build:static:staging": "nuxi generate --dotenv .env.staging",
    "web:build:static:prod": "nuxi generate --dotenv .env.production",
    "web:analyze": "nuxi analyze --loglevel verbose",
    "web:cleanup": "nuxi cleanup",
    "web:prepare": "nuxi prepare",
    "desktop:run:dev": "cross-env RUST_BACKTRACE=full tauri dev",
    "desktop:check:compile": "cd src-tauri && cargo check",
    "desktop:build:windows:x64": "tauri build --target x86_64-pc-windows-msvc",
    "desktop:build:windows:arm64": "tauri build --target aarch64-pc-windows-msvc",
    "desktop:build:macos:x86_64": "tauri build --target x86_64-apple-darwin",
    "desktop:build:macos:arm64": "tauri build --target aarch64-apple-darwin",
    "desktop:build:macos:universal": "tauri build --target universal-apple-darwin",
    "desktop:build:linux:x86_64": "tauri build --target x86_64-unknown-linux-gnu",
    "desktop:build:linux:arm64": "tauri build --target aarch64-unknown-linux-gnu",
    "desktop:install:target:windows": "rustup target add x86_64-pc-windows-msvc && rustup target add aarch64-pc-windows-msvc",
    "desktop:install:target:macos": "rustup target add aarch64-apple-darwin && rustup target add x86_64-apple-darwin",
    "desktop:install:target:linux": "rustup target add x86_64-unknown-linux-gnu && rustup target add aarch64-unknown-linux-gnu",
    "test:unit:dev": "vitest watch --coverage",
    "test:unit:dev:gui": "vitest watch --ui --coverage",
    "test:unit:staging-prod": "vitest run --coverage",
    "lint": "npm run lint:prettier && npm run lint:eslint && npm run lint:ts",
    "lint:prettier": "prettier --check .",
    "lint:eslint": "eslint .",
    "lint:ts": "vue-tsc --noEmit",
    "lint:fix": "prettier --write --list-different . && npm run lint:prettier --fix && npm run lint:eslint --fix",
    "tauri": "tauri",
    "prepare": "husky && nuxi prepare"
  },
  "dependencies": {
    "@pinia/nuxt": "^0.10.1",
    "@tauri-apps/api": "^2.3.0",
    "@tauri-apps/plugin-autostart": "^2.2.0",
    "@tauri-apps/plugin-dialog": "^2.2.0",
    "@tauri-apps/plugin-fs": "^2.2.0",
    "@tauri-apps/plugin-log": "^2.3.0",
    "@tauri-apps/plugin-notification": "^2.2.2",
    "@tauri-apps/plugin-os": "^2.2.1",
    "@tauri-apps/plugin-process": "^2.2.0",
    "@tauri-apps/plugin-shell": "^2.2.0",
    "@tauri-apps/plugin-updater": "^2.6.0",
    "@vee-validate/rules": "^4.15.0",
    "axios": "^1.8.2",
    "floating-vue": "^5.2.2",
    "js-base64": "^3.7.7",
    "luxon": "^3.5.0",
    "notyf": "^3.10.0",
    "pinia": "^3.0.1",
    "swiper": "^11.2.5",
    "vee-validate": "^4.15.0",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@nuxt/devtools": "^2.2.1",
    "@nuxt/test-utils": "^3.17.2",
    "@nuxtjs/google-fonts": "^3.2.0",
    "@nuxtjs/tailwindcss": "^6.13.2",
    "@stylistic/eslint-plugin": "^4.2.0",
    "@tauri-apps/cli": "^2.3.1",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/jsdom": "^21.1.7",
    "@types/luxon": "^3.4.2",
    "@types/node": "^22.13.10",
    "@typescript-eslint/eslint-plugin": "^8.26.1",
    "@typescript-eslint/parser": "^8.26.1",
    "@vitest/coverage-v8": "^3.0.8",
    "@vitest/ui": "^3.0.8",
    "@vue/test-utils": "^2.4.6",
    "autoprefixer": "^10.4.21",
    "cross-env": "^7.0.3",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-import-resolver-typescript": "^3.8.4",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsdoc": "^50.6.6",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-unused-imports": "^4.1.4",
    "eslint-plugin-vue": "^10.0.0",
    "husky": "^9.1.7",
    "init": "^0.1.2",
    "jsdom": "^26.0.0",
    "json": "^11.0.0",
    "nitropack": "^2.11.6",
    "npm-check-updates": "^17.1.15",
    "npx": "^10.2.2",
    "nuxt": "^3.16.0",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "sass": "^1.85.1",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2",
    "vitest": "^3.0.8",
    "vue-eslint-parser": "^10.1.1",
    "vue-tsc": "^2.2.8"
  },
  "engines": {
    "node": ">=22.14.0"
  }
}
